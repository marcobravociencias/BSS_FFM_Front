<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.totalplay.ffm.plantainterna.repository.OrdenTrabajoPIRepository">
 	
 	<select id="obtenerOrdenesTrabajoDespacho" resultType="com.totalplay.ffm.plantainterna.model.OrdenTrabajoPIVO"
		parameterType="java.util.Map">			
			SELECT
		    	EFO.EFO_EMPLOYEE_NUMBER ordenTrabajo
			   
			FROM ENL_FIELD_OPERATORS EFO
			WHERE 
			    EFO.EFO_ID <![CDATA[  <]]> 1000			
	</select>

</mapper>